{
  "name": "@arvin/ui-components",
  "version": "0.1.0",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:umd && npm run build:esm && npm run copy:index",
    "build:umd": "webpack --config webpack.config.ts --env mode=umd",
    "build:esm": "webpack --config webpack.config.ts --env mode=esm",
    "copy:index": "node -e \"const fs=require('fs');const path=require('path');fs.copyFileSync('dist/umd/index.js','dist/index.js');fs.existsSync('dist/umd/index.js.LICENSE.txt')&&fs.copyFileSync('dist/umd/index.js.LICENSE.txt','dist/index.js.LICENSE.txt');['dist/umd/index','dist/esm/index'].forEach(d=>{if(fs.existsSync(d))fs.rmSync(d,{recursive:true})})\""
  },
  "peerDependencies": {
    "antd": "^5.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/webpack-env": "^1.18.5",
    "antd": "^5.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
